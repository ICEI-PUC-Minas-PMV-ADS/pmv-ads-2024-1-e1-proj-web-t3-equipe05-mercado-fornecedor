async function buscarFornecedores() {
  const response = await fetch('db.json');
  const fornecedores = await response.json();
  return fornecedores;
}

async function buscarFornecedores() {
  const response = await fetch('db.json');
  const fornecedores = await response.json();

  const tabela = document.createElement('table');

  fornecedores.forEach(fornecedor => {
    const linha = document.createElement('tr');
    const nomeColuna = document.createElement('td');
    const enderecoColuna = document.createElement('td');
    const telefoneColuna = document.createElement('td');

    nomeColuna.innerText = fornecedor.nome;
    enderecoColuna.innerText = fornecedor.endereco;
    telefoneColuna.innerText = fornecedor.telefone;

    linha.appendChild(nomeColuna);
    linha.appendChild(enderecoColuna);
    linha.appendChild(telefoneColuna);
    tabela.appendChild(linha);
  });

  document.body.appendChild(tabela);
}